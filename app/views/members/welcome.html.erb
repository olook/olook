<% content_for :head do %>
  <%= stylesheet_link_tag "section/welcome" %>
<% end %>

<% content_for :footer do %>
  <%= javascript_include_tag "welcome" %>
  <%= render "shared/metrics/google/track_signup_page_view" %>
  <%= render "shared/metrics/google/sign_in_conversion" %>
  <%= render "shared/metrics/metaffiliation/action", :email => current_user.email %>
  <%= render "shared/metrics/adlead/adlead" %>
  <%= render "shared/metrics/yahoo/register" %>
  <%= render partial: "shared/metrics/dbm/quiz" %>
<% end %>

<section id="welcome">
  <%= render "shared/modal/retake_quiz", :user => @user, :link => member_showroom_path %>
</section>
