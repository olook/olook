<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
      <%= link_to "Sign in with #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider), :class => "fb-connect fbpopup", :"data-width" => 600, :"data-height" => 400 %>
  <% end -%>
<% end -%>
