<script type="text/javascript">
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', "<%= ANALYTICS_CONFIG['analytics_id'] %>" ]);
  _gaq.push(['_setCustomVar',1,'Member Type','<%= member_type %>',1]);
  <% if user_signed_in? %>
    _gaq.push(['_setCustomVar',2,'Age','<%= current_user.age %>',1]);
    _gaq.push(['_setCustomVar',3,'Profile','<%= current_user.profile_name %>',1]);
  <% end %>
  _gaq.push(['_trackPageview']);
  <%= yield :ecommerce_tracking %>
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script>
<script type="text/javascript" src="https://s3.amazonaws.com/ha_js/ga_social_tracking.js"></script>
